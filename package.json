{
    "name": "daskeyboard-applet--fly-me",
    "version": "1.0.0",
    "displayName": "Fly Me",
    "description": "Find the best prices and throw yourself in the air with Fly Me.",
    "longDescription": "Displays a notification on Das Keyboard Q series when there is a cheap flight.",
    "officialProductName": "Fly Me",
    "author": "Fly Me",
    "appUrl": "",
    "license": "MIT",
    "icon": "assets/icon.png",
    "image": "assets/image.png",
    "authorName": "Metadot Corporation",
    "authorUrl": "https://twitter.com/daskeyboard",
    "issuesUrl": "https://github.com/daskeyboard/daskeyboard-applet--fly-me/issues",
    "homePageUrl": "https://twitter.com/daskeyboard",
    "developerRepoUrl": "https://github.com/daskeyboard/daskeyboard-applet--fly-me",
    "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
    "publisher": "Metadot Corporation",
    "changelogUrl": "CHANGELOG.md",
    "readMeUrl": "README.md",
    "readMeEndUserUrl": "README_END_USER.md",
    "isSingleton": false,
    "main": "index.js",
    "scripts": {
      "test": "mocha"
    },
    "engines": {
      "das-keyboard-q": "3.2.6"
    },
    "qConfig": {
      "geometry": {
        "width": 1,
        "height": 1
      },
      "authorization": {
        "type": "apiKey",
        "hint": "Get the api key from the search flight page of Skyscanner website.",
        "supportUrl": "https://skyscanner-skyscanner-flight-search-v1.p.rapidapi.com/apiservices/pricing/v1.0"
      },
      "questions": [
        {
          "key": "DEPART_DATE",
          "label": "Choose an depart date.",
          "help": "yyyy-mm-dd",
          "required": true,
          "order": 1,
          "controlType": "textbox",
          "dynamic": true
        },
        {
          "key": "RETURN_DATE",
          "label": "Choose an return date.",
          "help": "yyyy-mm-dd",
          "required": true,
          "order": 1,
          "controlType": "textbox",
          "dynamic": true
        },
        {
          "key": "ORIGIN_PLACE",
          "label": "Choose an origin place.",
          "help": "",
          "required": true,
          "order": 1,
          "controlType": "textbox",
          "dynamic": true
        },
        {
          "key": "DESTINATION_PLACE",
          "label": "Choose an destination place.",
          "help": "",
          "required": true,
          "order": 1,
          "controlType": "textbox",
          "dynamic": true
        },
        {
          "key": "PRICE_EXPECTED",
          "label": "Enter your expected price.",
          "help": "",
          "required": true,
          "order": 1,
          "controlType": "textbox",
          "dynamic": true
        }]
    },
    "dependencies": {
      "daskeyboard-applet": "^2.11.4",
      "request-promise": "^4.2.5"
    },
    "devDependencies": {
      "mocha": "^6.2.2"
    }
  }